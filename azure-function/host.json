{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
          "Function.checkfsquota": "Information",
          "Function.TimerTrigger1": "Information",
          "default": "Information"
        },
    "applicationInsights": {
